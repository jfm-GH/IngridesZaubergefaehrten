{
  "name": "ingridetheartist",
  "version": "6.1.0",
  "private": true,
  "description": "This is about the simplest template you can get for uibuilder.",
  "browser": "./src/index.js",
  "scripts": {
    "encode-blur-hash": "node ./src/scripts/encodeBlurHash.js",
    "prebuild": "node ./src/scripts/prebuild.js --build",
    "preserve": "node ./src/scripts/prebuild.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postbuild": "node ./src/scripts/postbuild.js",
    "lint": "vue-cli-service lint",
    "deploy": "cd dist && cp index.html 404.html"
  },
  "keywords": [
    "uibuilder",
    "node-red",
    "node-red-contrib-uibuilder"
  ],
  "author": "Julian Knight (Totally Information)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/TotallyInformation/node-red-contrib-uibuilder",
  "bugs": "https://github.com/TotallyInformation/node-red-contrib-uibuilder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/TotallyInformation/node-red-contrib-uibuilder.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@popperjs/core": "2.10.2",
    "blurhash": "^2.0.5",
    "bootstrap": "5.1.3",
    "canvas": "^2.11.2",
    "chart.js": "3.6.0",
    "core-js": "3.6.5",
    "dropzone": "6.0.0-beta.1",
    "md5": "^2.3.0",
    "mdb-ui-kit": "^7.0.0",
    "quill": "1.3.6",
    "readline-sync": "^1.4.10",
    "vue": "3.2.0",
    "vue-flatpickr-component": "9.0.5",
    "vue-router": "4.0.14",
    "vue3-touch-events": "^4.1.8",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-router": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.2.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "animate-sass": "^0.8.2",
    "babel-eslint": "10.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.0.0",
    "prettier": "2.2.1",
    "sass": "1.43.3",
    "sass-loader": "10.1.1"
  }
}
